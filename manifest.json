{
    "manifest_version": 2,
    "name": "RReminder",
    "description": "Simple Reminder",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-2.2.4.min.js",
                "script.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": {
            "16": "images/favicon16.png",
            "24": "images/favicon24.png",
            "32": "images/favicon32.png"
        }
    }
}